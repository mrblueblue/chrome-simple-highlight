{
  "manifest_version": 2,

  "name": "Annotate",
  "description": "A simple text annotator",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Annotate"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}